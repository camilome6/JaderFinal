        <TextView
            android:id="@+id/objetoDelcontr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_x="184dp"
            android:layout_y="132dp"
            android:text="Datos Open" />


    </AbsoluteLayout>
</android.support.v7.widget.CardView>
